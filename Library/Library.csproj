<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>Webql</RootNamespace>
    <PackageId>Aidan.Webql</PackageId>
    <Title>A query language that compiles to LINQ expressions</Title>
    <Version>1.0.0-alpha</Version>
    <Authors>Rodrigo Pazzini Jacques</Authors>
    <Company>Aidan</Company>
    <AssemblyName>Aidan.Webql</AssemblyName>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Aidan.Core" Version="1.0.1" />
    <PackageReference Include="Aidan.TextAnalysis" Version="1.0.1" />
  </ItemGroup>

</Project>
